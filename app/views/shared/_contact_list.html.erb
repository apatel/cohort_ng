<h1><%=h title %></h1>
<% unless @contacts.blank? %>
  <ul>
    <% @contacts.each do |c| %>
      <li id="contact-<%= c.id %>"><%=h c.first_name %> <%=h c.last_name %><span class="floating-control control-contact-<%= c.id %>"><%= link_to('edit',edit_contact_path(c),:class => 'dialog') %> <%= link_to('del',contact_path(c), :method => :delete, :class => 'delete') %></span></li>
    <% end %>
  </ul>
  <%= will_paginate(@contacts) %>
<% else %>
  <h2>None found.</h2>
<% end %>
