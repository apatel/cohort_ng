<% unless title.blank? %>
  <h1><%= title %></h1>
<% end %>
<% unless @contact_carts.blank? %>
  <ul class="resultlist">
    <%= render :partial => 'shared/lines/contact_cart', :collection => @contact_carts %>
  </ul>
  <%= will_paginate(@contact_carts) %>
<% else %>
  <h2>None found.</h2>
<% end %>
