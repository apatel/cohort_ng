<li class="contact-cart-<%= contact_cart.id %>">
<% unless current_user.blank? %>
  <a href="#" class="control" id="contact_cart-control-<%= contact_cart.id %>"><%= icon('add') %></a>
<% end %>
<%= link_to(contact_cart, contact_cart, :class => 'dialog-show') %>
<% unless current_user.blank? %>
  <span id="contact_cart-control-<%= contact_cart.id %>-target" style="display: none;"><%= link_to(icon('note_edit') + ' Edit contact list', edit_contact_cart_path(contact_cart),:class => 'dialog-form', :title => 'Edit contact list') %><br/><%= link_to(icon('note_delete') + ' Delete note',contact_cart_path(contact_cart), :method => :delete, :title => 'Delete contact list', :class => 'delete delete-contact_cart') %></span>
<% end %>
</li>
